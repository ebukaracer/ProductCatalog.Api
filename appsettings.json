{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "Database": {
    "RunMigration": true,
    "SeedInitialData": true
  },
  "Exception": {
    "ShowDetails": false
  },
  "AllowedHosts": "*"
}
